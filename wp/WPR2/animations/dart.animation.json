{
	"format_version": "1.8.0",
	"animations": {
		"animation.dart.spin": {
			"loop": true,
			"animation_length": 0.5,
			"bones": {
				"body": {
					"rotation": {
						"0.0": [0, 0, 0],
						"0.5": [360, 0, 0]
					}
				}
			}
		},
		"animation.dart.face" : {
			"loop" : true,
			"bones" : {
				"body" : {
					"rotation" : [ "variable.shake_power - query.target_x_rotation", "-query.target_y_rotation", 0.0 ]
				}
			}
		}
	}
}