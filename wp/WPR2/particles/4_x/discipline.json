{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "wb:discipline_par",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/wb_par/discipline"
			}
		},
		"components": {
			"minecraft:emitter_local_space": {
				"position": true,
				"rotation": true
			},
			"minecraft:particle_motion_parametric": {
				"relative_position": [
					"variable.particle_age > 1.5 ? (variable.particle_age > 2.0 ? 0.0 : Math.sin(variable.particle_random_1*360)*2) : Math.sin(variable.particle_random_1*360)*5",
					"variable.particle_age > 1.5 ? (variable.particle_age > 2.0 ? 25-(variable.particle_age-2)*15 : 6.0) : 7.0",
					"variable.particle_age > 1.5 ? (variable.particle_age > 2.0 ? 0.0 : Math.cos(variable.particle_random_1*360)*2) : Math.cos(variable.particle_random_1*360)*5"
				]
			},
			"minecraft:emitter_rate_steady": {
				"spawn_rate": 120,
				"max_particles": 180
			},
			"minecraft:emitter_lifetime_looping": {
				"active_time": 1.5
			},
			"minecraft:emitter_shape_point": {},
			"minecraft:particle_initial_speed": "(Math.Random(0, 1) + Math.Random(0, 1) + 1) * 0.8",
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "Math.Random(4.2,4.8)"
			},
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": [
					0,
					0.02,
					0
				]
			},
			"minecraft:particle_motion_collision": {
				"collision_radius": 0.001
			},
			"minecraft:particle_appearance_billboard": {
				"size": [
					0.25,
					0.25
				],
				"facing_camera_mode": "lookat_xyz",
				"uv": {
					"texture_width": 2,
					"texture_height": 1,
					"flipbook": {
						"base_UV": [
							0,
							0
						],
						"size_UV": [
							1,
							1
						],
						"step_UV": [
							1,
							0
						],
						"frames_per_second": 2,
						"max_frame": 2,
						"stretch_to_lifetime": true,
						"loop": false
					}
				}
			}
		}
	}
}