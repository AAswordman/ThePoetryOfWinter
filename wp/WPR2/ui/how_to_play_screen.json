{
  "namespace": "how_to_play",
  "how_to_play_screen@how_to_play_common.screen_base": {
    "$selector_stack_panel": "how_to_play.selector_stack_panel",
    "$section_content_panels": "how_to_play.section_content_panels"
  },
  "selector_stack_panel": {
    "type": "stack_panel",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "orientation": "vertical",
    "$default_selector_toggle_index": 1,
    "controls": [
      {
        "how_to_play_selector_pane": {
          "type": "stack_panel",
          "controls": [
            {
              "padding_0": {
                "type": "panel",
                "size": [0,6]
              }
            },
            {
              "wb0_button@how_to_play.wb0_button": {
                "$toggle_group_forced_index": 0
              }
            },
            {
              "wb0x_button@how_to_play.wb0x_button": {
                "$toggle_group_forced_index": 1
              }
            },
            {
              "all@how_to_play_common.topic_category": {
                "$category": "all"
              }
            },
            {
              "wb1_button@how_to_play.wb1_button": {
                "$toggle_group_forced_index": 2
              }
            },
            {
              "core@how_to_play_common.topic_category": {
                "$category": "core"
              }
            },
            {
              "wb2_button@how_to_play.wb2_button": {
                "$toggle_group_forced_index": 3
              }
            },
            
            {
              "item@how_to_play_common.topic_category": {
                "$category": "item"
              }
            },
            {
              "wb3_button@how_to_play.wb3_button": {
                "$toggle_group_forced_index": 5
              }
            },
            {
              "wb4_button@how_to_play.wb4_button": {
                "$toggle_group_forced_index": 6
              }
            },
            {
              "wb5_button@how_to_play.wb5_button": {
                "$toggle_group_forced_index": 7
              }
            },
            {
              "wb6_button@how_to_play.wb6_button": {
                "$toggle_group_forced_index": 8
              }
            },
            {
              "tech@how_to_play_common.topic_category": {
                "$category": "tech"
              }
            },
            {
              "wb7_button@how_to_play.wb7_button": {
                "$toggle_group_forced_index": 9
              }
            },
            {
              "plot@how_to_play_common.topic_category": {
                "$category": "plot"
              }
            },
            {
              "wb8_button@how_to_play.wb8_button": {
                "$toggle_group_forced_index": 10
              }
            }
          ]
        }
      }
    ]
  },
  "section_content_panels": {
