    "size": [
      "100% - 5px",
      "100%c + 5px"
    ],
    "offset": [2,0],
    "controls": [
      {
        "general_tips_sections": {
          "type": "stack_panel",
          "controls": [
            {
              "wb0_section@how_to_play.wb0_section": {}
            },
            {
              "wb0x_section@how_to_play.wb0x_section": {}
            },
            {
              "wb1_section@how_to_play.wb1_section": {}
            },
            {
              "wb2_section@how_to_play.wb2_section": {}
            },
            
            {
              "wb3_section@how_to_play.wb3_section": {}
            },
            {
              "wb4_section@how_to_play.wb4_section": {}
            },
            {
              "wb5_section@how_to_play.wb5_section": {}
            },
            {
              "wb6_section@how_to_play.wb6_section": {}
            },
            {
              "wb7_section@how_to_play.wb7_section": {}
            },
            {
              "wb8_section@how_to_play.wb8_section": {}
            }
          ]
        }
      }
    ]
  },
  "wb0_button@how_to_play_common.section_toggle_button": {
    "$section_topic": "wb0"
  },
  "wb0x_button@how_to_play_common.section_toggle_button": {
    "$section_topic": "wb0x"
  },
  "wb1_button@how_to_play_common.section_toggle_button": {
    "$section_topic": "wb1"
  },
  "wb2_button@how_to_play_common.section_toggle_button": {
    "$section_topic": "wb2"
  },
  
  "wb3_button@how_to_play_common.section_toggle_button": {
    "$section_topic": "wb3"
  },
  "wb4_button@how_to_play_common.section_toggle_button": {
    "$section_topic": "wb4"
  },
  "wb5_button@how_to_play_common.section_toggle_button": {
    "$section_topic": "wb5"
  },
  "wb6_button@how_to_play_common.section_toggle_button": {
    "$section_topic": "wb6"
  },
  "wb7_button@how_to_play_common.section_toggle_button": {
    "$section_topic": "wb7"
  },
  "wb8_button@how_to_play_common.section_toggle_button": {
