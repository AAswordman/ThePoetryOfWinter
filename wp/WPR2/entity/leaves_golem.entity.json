{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "dec:leaves_golem",
      "materials": { "default": "spider" },
      "textures": {
        "default": "textures/entity/leaves_golem"
      },
      "spawn_egg": {
        "base_color": "#548B54",
        "overlay_color": "#556B2F"
      },
      "scripts": {
        "animate": [
          "spawn",
          {"roar" : "query.is_roaring > 0"},
          {"walk" : "query.modified_move_speed"},
          {"attacking":"variable.attack_animation_tick > 0.0"},
          "look_at_target"
        ]
      },
      "geometry": {
        "default": "geometry.leaves_golem"
      },
      "animations": {
        "spawn": "animation.leaves_golem.spawn",
        "roar": "animation.leaves_golem.roar",
        "look_at_target": "animation.common.look_at_target",
        "walk": "animation.leaves_golem.move",
        "attacking": "animation.leaves_golem.attack"
      },
      "render_controllers": [ "controller.render.spider" ]
    }
  }
}