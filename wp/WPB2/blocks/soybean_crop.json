{
    "format_version": "1.16.0",
    "minecraft:block": {
        "description": {
            "identifier": "dec:soybean_crop",
            "properties": {
                "dec:growth_stage": [
                    0,
                    1,
                    2,
                    3,
                    4
                ]
            }
        },
        "components": {
            "minecraft:geometry": "geometry.crop.hash",
            "minecraft:entity_collision": false,
            "minecraft:pick_collision": {
                "origin": [
                    -8,
                    0,
                    -8
                ],
                "size": [
                    16,
                    16,
                    16
                ]
            },
            "minecraft:destroy_time": 0.001,
            "minecraft:friction": 0.2,
            "minecraft:map_color": "#8FE08B",
            "minecraft:block_light_absorption": 0.0,
            "minecraft:loot": "loot_tables/blocks/soybean_crop.json",
            "minecraft:on_interact": {
                "event": "meal",
                "condition": "query.get_equipped_item_name('main_hand') == 'bone_meal'"
            },
            "minecraft:random_ticking": {
                "on_tick": {
                    "event": "grow_stage"
                }
            },
            "minecraft:placement_filter": {
                "conditions": [
                    {
                        "allowed_faces": [
                            "up"
                        ],
                        "block_filter": [
                            "farmland",
                            "podzol"
                        ]
                    }
                ]
            }
        },
        "events": {
            "grow_stage": {
                "randomize": [
                    {
                        "weight": 1
                    },
                    {
                        "weight": 1,
                        "set_block_property": {
                            "dec:growth_stage": "(query.block_property('dec:growth_stage')<4) ? query.block_property('dec:growth_stage') + 1 : 4"
                        }
                    }
                ]
            },
            "final_drop": {
                "spawn_loot": {
                    "table": "loot_tables/blocks/soybean_crop.json"
                },
                "set_block": {
                    "block_type": "dec:soybean_crop"
                }
            },
            "meal": {
                "sequence": [
                    {
                        "decrement_stack": {}
                    },
                    {
                        "run_command": {
                            "command": [
                                "particle minecraft:crop_growth_emitter ~~~"
                            ]
                        }
                    },
                    {
                        "trigger": {
                            "event": "grow_stage"
                        }
                    }
                ]
            }
        },
        "permutations": [
            {
                "condition": "query.block_property('dec:growth_stage') == 0",
                "components": {
                    "minecraft:material_instances": {
                        "*": {
                            "texture": "soybean_crop_0",
                            "render_method": "alpha_test"
                        }
                    },
                    "minecraft:pick_collision": {
                        "origin": [
                            -7,
                            0,
                            -7
                        ],
                        "size": [
                            14,
                            3,
                            14
                        ]
                    },
                    "minecraft:loot": "loot_tables/empty.json"
                }
            },
            {
                "condition": "query.block_property('dec:growth_stage') == 1",
                "components": {
                    "minecraft:material_instances": {
                        "*": {
                            "texture": "soybean_crop_1",
                            "render_method": "alpha_test"
                        }
                    },
                    "minecraft:pick_collision": {
                        "origin": [
                            -7,
                            0,
                            -7
                        ],
                        "size": [
                            14,
                            4,
                            14
                        ]
                    },
                    "minecraft:loot": "loot_tables/empty.json"
                }
            },
            {
                "condition": "query.block_property('dec:growth_stage') == 2",
                "components": {
                    "minecraft:material_instances": {
                        "*": {
                            "texture": "soybean_crop_2",
                            "render_method": "alpha_test"
                        }
                    },
                    "minecraft:pick_collision": {
                        "origin": [
                            -6.5,
                            0,
                            -6.5
                        ],
                        "size": [
                            13,
                            9,
                            13
                        ]
                    },
                    "minecraft:loot": "loot_tables/empty.json"
                }
            },
            {
                "condition": "query.block_property('dec:growth_stage') == 3",
                "components": {
                    "minecraft:material_instances": {
                        "*": {
                            "texture": "soybean_crop_3",
                            "render_method": "alpha_test"
                        }
                    },
                    "minecraft:pick_collision": {
                        "origin": [
                            -8,
                            0,
                            -8
                        ],
                        "size": [
                            16,
                            11,
                            16
                        ]
                    },
                    "minecraft:loot": "loot_tables/empty.json"
                }
            },
            {
                "condition": "query.block_property('dec:growth_stage') == 4",
                "components": {
                    "minecraft:material_instances": {
                        "*": {
                            "texture": "soybean_crop_4",
                            "render_method": "alpha_test"
                        }
                    },
                    "minecraft:pick_collision": {
                        "origin": [
                            -8,
                            0,
                            -8
                        ],
                        "size": [
                            16,
                            13,
                            16
                        ]
                    },
                    "minecraft:loot": "loot_tables/empty.json",
                    "minecraft:on_interact": {
                        "event": "final_drop",
                        "target": "self"
                    }
                }
            }
        ]
    }
}