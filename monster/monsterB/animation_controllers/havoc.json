{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.takile.havoc.circuitous": {
      "states": {
        "default": {
          "on_entry": [
            "variable.num = 0;"
          ],
          "transitions": [
            {
              "start": "variable.num == 0"
            }
          ]
        },
        "start": {
          "on_entry": [
            "/say gj"
          ],
          "transitions": [
            {
              "attack": "query.is_charged && variable.num < 7"
            },
            {
              "attack2": "query.is_charged && variable.num >= 7"
            }
          ]
        },
        "attack": {
          "on_entry": [
            "/say gj2",
            "variable.num = 1 + variable.num;",
            "/summon takile:havoc_proj ~ ~ ~ instantdamage_2",
            "@s skill:circuitous_attack_start2"
          ],
          "transitions": [
            {
              "start": "!query.is_charged"
            }
          ]
        },
        "attack2": {
          "on_entry": [
            "/summon takile:havoc_proj ~ ~ ~ instantdamage_2",
            "@s skill:circuitous_attack_start"
          ],
          "transitions": [
            {
              "start": "!query.is_charged"
            }
          ]
        }
      }
    },
    "controller.animation.takile.havoc.coordination.a": {
      "states": {
        "default": {
          "transitions": [
            {
              "attack": "(1.0)"
            }
          ]
        },
        "attack": {
          "on_entry": [
            "/effect @e[type=minecraft:player,r=5] strength 6 1",
            "/effect @s strength 6 1"
          ],
          "transitions": [
            {
              "default": "(1.0)"
            }
          ]
        }
      }
    },
    "controller.animation.takile.havoc.coordination": {
      "states": {
        "default": {
          "transitions": [
            {
              "attack": "query.is_delayed_attacking"
            }
          ]
        },
        "attack": {
          "on_entry": [
            "/execute @e[type=!minecraft:player,r=6] ~ ~ ~ summon takile:havoc_proj ~ ~ ~ damage_60"
          ],
          "transitions": [
            {
              "default": "!query.is_delayed_attacking"
            }
          ]
        }
      }
    },
    "controller.animation.takile.havoc.strike": {
      "states": {
        "default": {
          "transitions": [
            {
              "attack": "query.is_delayed_attacking"
            }
          ]
        },
        "attack": {
          "on_entry": [
            "variable.test = Math.random(0,1);"
          ],
          "transitions": [
            {
              "default": "!query.is_delayed_attacking"
            },
            {
              "attack2": "query.is_delayed_attacking && variable.test > 0.5"
            }
          ]
        },
        "attack2": {
          "on_entry": [
            "/execute @e[r=6,c=1,type=!takile:havoc_monster] ~ ~ ~ tp @s ^ ^+4 ^-7"
          ],
          "transitions": [
            {
              "default": "!query.is_delayed_attacking"
            }
          ]
        }
      }
    }
  }
}