{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.takile.havoc.circuitous": {
      "states": {
        "default": {
          "transitions": [
            {
              "attack": "query.is_delayed_attacking"
            }
          ]
        },
        "attack": {
          "on_entry": [
            "/summon takile:havoc_proj ~ ~ ~ instantdamage_2",
            "/event entity @s skill:circuitous_attack"
          ],
          "transitions": [
            {
              "default": "!query.is_delayed_attacking"
            }
          ]
        }
      }
    },
    "controller.animation.takile.havoc.coordination.a": {
      "states": {
        "default": {
          "transitions": [
            {
              "attack": "(1.0)"
            }
          ]
        },
        "attack": {
          "on_entry": [
            "/effect @e[type=minecraft:player,r=5] ~ ~ ~ effect @s strength 6 1"
          ],
          "transitions": [
            {
              "default": "(1.0)"
            }
          ]
        }
      }
    },
    "controller.animation.takile.havoc.coordination": {
      "states": {
        "default": {
          "transitions": [
            {
              "attack": "query.is_delayed_attacking"
            }
          ]
        },
        "attack": {
          "on_entry": [
            "/execute @e[type=!minecraft:player,r=6] ~ ~ ~ summon takile:havoc_proj ~ ~ ~ damage_60"
          ],
          "transitions": [
            {
              "default": "!query.is_delayed_attacking"
            }
          ]
        }
      }
    },
    "controller.animation.takile.havoc.strike": {
      "states": {
        "default": {
          "transitions": [
            {
              "attack": "query.is_delayed_attacking"
            }
          ]
        },
        "attack": {
          "on_entry": [
            "variable.test = Math.random(0,1);"
          ],
          "transitions": [
            {
              "default": "!query.is_delayed_attacking"
            },
            {
              "attack2": "query.is_delayed_attacking && variable.test > 0.4"
            }
          ]
        },
        "attack2": {
          "on_entry": [
            "/execute @e[r=6,c=1,type=!takile:havoc_monster] ~ ~ ~ tp @s ^ ^+3 ^-4"
          ],
          "transitions": [
            {
              "default": "!query.is_delayed_attacking"
            }
          ]
        }
      }
    }
  }
}