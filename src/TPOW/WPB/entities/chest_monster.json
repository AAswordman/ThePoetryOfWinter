{
  "format_version": "1.16.0",
  "minecraft:entity": {
    "description": {
      "identifier": "dec:chest_monster",
      "is_spawnable": true,
      "is_summonable": true,
      "is_experimental": false
    },
    "component_groups": {
      "minecraft:despawn": {
        "minecraft:instant_despawn": {}
      },
      "minecraft:escape_successfully": {
        "minecraft:timer": {
          "looping": true,
          "time": 1.7,
          "time_down_event": {
            "event": "minecraft:despawn"
          }
        }
      },
      "minecraft:escape": {
        "minecraft:target_nearby_sensor": {
          "outside_range": 11,
          "must_see": true,
          "on_outside_range": {
            "event": "minecraft:escape_successfully",
            "target": "self"
          },
          "on_vision_lost_inside_range": {
            "event": "minecraft:escape_successfully",
            "target": "self"
          }
        },
        "minecraft:loot": {
          "table": "loot_tables/entities/chest_monster.json"
        },
        "minecraft:behavior.avoid_mob_type": {
          "priority": 5,
          "entity_types": [
            {
              "filters": {
                "test": "is_family",
                "subject": "other",
                "value": "player"
              },
              "must_see": false,
              "max_dist": 64,
              "walk_speed_multiplier": 1,
              "sprint_speed_multiplier": 1.4
            }
          ]
        }
      },
      "minecraft:before_trade": {
        "minecraft:timer": {
          "looping": false,
          "time": 3,
          "time_down_event": {
            "event": "minecraft:trade"
          }
        }
      },
      "minecraft:can_trade": {
        "minecraft:interact": {
          "interactions": [
            {
              "on_interact": {
                "filters": {
                  "all_of": [
                    {
                      "any_of": [
                        {
                          "test": "has_equipment",
                          "subject": "other",
                          "domain": "hand",
                          "value": "minecraft:gold_block"
                        },
                        {
                          "test": "has_equipment",
                          "subject": "other",
                          "domain": "hand",
                          "value": "minecraft:raw_gold_block"
                        },
                        {
                          "test": "has_equipment",
                          "subject": "other",
                          "domain": "hand",
                          "value": "minecraft:iron_block"
                        },
                        {
                          "test": "has_equipment",
                          "subject": "other",
                          "domain": "hand",
                          "value": "minecraft:raw_iron_block"
                        },
                        {
                          "test": "has_equipment",
                          "subject": "other",
                          "domain": "hand",
                          "value": "minecraft:lapis_block"
                        },
                        {
                          "test": "has_equipment",
                          "subject": "other",
                          "domain": "hand",
                          "value": "minecraft:coal_block"
                        },
                        {
                          "test": "has_equipment",
                          "subject": "other",
                          "domain": "hand",
                          "value": "minecraft:redstone_block"
                        },
                        {
                          "test": "has_equipment",
                          "subject": "other",
                          "domain": "hand",
                          "value": "minecraft:diamond_block"
                        },
                        {
                          "test": "has_equipment",
                          "subject": "other",
                          "domain": "hand",
                          "value": "minecraft:copper_block"
                        },
                        {
                          "test": "has_equipment",
                          "subject": "other",
                          "domain": "hand",
                          "value": "minecraft:raw_copper_block"
                        },
                        {
                          "test": "has_equipment",
                          "subject": "other",
                          "domain": "hand",
                          "value": "minecraft:emerald_block"
                        },
                        {
                          "test": "has_equipment",
                          "subject": "other",
                          "domain": "hand",
                          "value": "minecraft:netherite_block"
                        }
                      ]
                    },
                    {
                      "test": "is_family",
                      "subject": "other",
                      "value": "player"
                    },
                    {
                      "test": "has_component",
                      "subject": "self",
                      "operator": "!=",
                      "value": "minecraft:is_baby"
                    }
                  ]
                },
                "event": "minecraft:trade_before"
              },
              "barter": true,
              "use_item": true,
              "cooldown_after_being_attacked": 20,
              "interact_text": "action.interact.barter"
            }
          ]
        }
      },
      "minecraft:calm": {
        "minecraft:damage_sensor": {
          "triggers": [
            {
              "on_damage": {
                "event": "minecraft:become_angry"
              }
            }
          ]
        },
        "minecraft:target_nearby_sensor": {
          "inside_range": 0.4,
          "must_see": true,
          "on_inside_range": {
            "event": "minecraft:become_angry",
            "target": "self"
          }
        }
      },
      "minecraft:angry": {
        "minecraft:target_nearby_sensor": {
          "outside_range": 13,
          "must_see": true,
          "on_outside_range": {
            "event": "minecraft:become_calm",
            "target": "self"
          },
          "on_vision_lost_inside_range": {
            "event": "minecraft:become_calm",
            "target": "self"
          }
        },
        "minecraft:loot": {
          "table": "loot_tables/entities/chest_monster.json"
        },
        "minecraft:behavior.melee_attack": {
          "priority": 4,
          "speed_multiplier": 1.0,
          "track_target": true
        },
        "minecraft:attack": {
          "damage": 7
        }
      }
    },
    "components": {
      "minecraft:despawn": {
        "despawn_from_distance": {}
      },
      "minecraft:behavior.nearest_attackable_target": {
        "priority": 2,
        "reselect_targets": true,
        "entity_types": [
          {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "value": "player"
            },
            "max_dist": 64
          },
          {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "value": "villager"
            },
            "max_dist": 64
          },
          {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "value": "irongolem"
            },
            "max_dist": 64
          }
        ],
        "must_see": true
      },
      "minecraft:knockback_resistance": {
        "value": 1.0
      },
      "minecraft:nameable": {},
      "minecraft:type_family": {
        "family": [
          "chest_monster",
          "monster",
          "undead"
        ]
      },
      "minecraft:breathable": {
        "total_supply": 15,
        "suffocate_time": 0
      },
      "minecraft:collision_box": {
        "width": 1,
        "height": 1
      },
      "minecraft:health": {
        "value": {
          "range_min": 30,
          "range_max": 50
        }
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": {
              "test": "in_lava",
              "subject": "self",
              "operator": "==",
              "value": true
            },
            "cause": "lava",
            "damage_per_tick": 4
          }
        ]
      },
      "minecraft:movement": {
        "value": 0.4
      },
      "minecraft:navigation.walk": {
        "can_path_over_water": false,
        "avoid_damage_blocks": true
      },
      "minecraft:movement.basic": {},
      "minecraft:jump.static": {},
      "minecraft:can_climb": {},
      "minecraft:behavior.float": {
        "priority": 0
      },
      "minecraft:behavior.mount_pathing": {
        "priority": 1,
        "speed_multiplier": 1.25,
        "target_dist": 64,
        "track_target": true
      },
      "minecraft:behavior.stay_while_sitting": {
        "priority": 3
      },
      "minecraft:physics": {},
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      }
    },
    "events": {
      "minecraft:entity_spawned": {
        "add": {
          "component_groups": [
            "minecraft:calm",
            "minecraft:can_trade"
          ]
        }
      },
      "minecraft:become_calm": {
        "remove": {
          "component_groups": [
            "minecraft:angry"
          ]
        },
        "add": {
          "component_groups": [
            "minecraft:calm",
            "minecraft:can_trade"
          ]
        }
      },
      "minecraft:become_angry": {
        "add": {
          "component_groups": [
            "minecraft:angry"
          ]
        },
        "remove": {
          "component_groups": [
            "minecraft:calm",
            "minecraft:can_trade"
          ]
        },
        "run_command": {
          "command": [
            "particle minecraft:villager_angry ~~1~"
          ],
          "target": "self"
        }
      },
      "minecraft:trade_before": {
        "add": {
          "component_groups": [
            "minecraft:before_trade"
          ]
        },
        "remove": {
          "component_groups": [
            "minecraft:can_trade"
          ]
        }
      },
      "minecraft:trade": {
        "randomize": [
          {
            "weight": 1,
            "add": {
              "component_groups": [
                "minecraft:angry"
              ]
            },
            "remove": {
              "component_groups": [
                "minecraft:can_trade",
                "minecraft:before_trade"
              ]
            },
            "run_command": {
              "command": [
                "particle minecraft:villager_angry ~~1~"
              ],
              "target": "self"
            }
          },
          {
            "weight": 3,
            "add": {
              "component_groups": [
                "minecraft:escape"
              ]
            },
            "remove": {
              "component_groups": [
                "minecraft:can_trade",
                "minecraft:before_trade"
              ]
            }
          },
          {
            "weight": 4,
            "add": {
              "component_groups": [
                "minecraft:can_trade"
              ]
            },
            "run_command": {
              "command": [
                "loot spawn ~~1~ loot \"entities/chest_monster_trade\"",
                "playanimation @s animation.chest_monster.trade_successfully"
              ],
              "target": "self"
            },
            "remove": {
              "component_groups": [
                "minecraft:before_trade"
              ]
            }
          }
        ]
      },
      "minecraft:escape_successfully": {
        "add": {
          "component_groups": [
            "minecraft:escape_successfully"
          ]
        },
        "remove": {
          "component_groups": [
            "minecraft:escape"
          ]
        },
        "run_command": {
          "command": [
            "playanimation @s animation.chest_monster.escape_successfully"
          ],
          "target": "self"
        }
      },
      "minecraft:despawn": {
        "add": {
          "component_groups": [
            "minecraft:despawn"
          ]
        }
      }
    }
  }
}