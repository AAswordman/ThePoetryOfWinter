{
  "format_version": "1.16.0",
  "minecraft:entity": {
    "description": {
      "identifier": "dec:frozen_ghast",
      "runtime_identifier": "minecraft:ghast",
      "is_spawnable": true,
      "is_summonable": true,
      "is_experimental": false
    },
    "component_groups": {
      "minecraft:from_boss": {
        "minecraft:explode": {
          "fuseLength": 30,
          "fuseLit": true,
          "power": 0,
          "causesFire": false
        }
      },
      "minecraft:despawn": {
        "minecraft:instant_despawn": {}
      },
      "minecraft:from_night_event": {
        "minecraft:environment_sensor": {
          "triggers": {
            "filters": {
              "test": "is_daytime",
              "subject": "self",
              "operator": "==",
              "value": true
            },
            "event": "minecraft:despawn"
          }
        }
      },
      "minecraft:repeat": {
        "minecraft:timer": {
          "looping": false,
          "time": 5,
          "time_down_event": {
            "event": "minecraft:single"
          }
        },
        "minecraft:shooter": {
          "def": "dec:frozen_ball_inaccurated"
        },
        "minecraft:behavior.ranged_attack": {
          "priority": 4,
          "burst_shots": 4,
          "burst_interval": 0.01,
          "charge_charged_trigger": 0.0,
          "charge_shoot_trigger": 4.0,
          "attack_interval_min": 2.0,
          "attack_interval_max": 2.0,
          "attack_radius": 64.0
        }
      },
      "minecraft:single": {
        "minecraft:timer": {
          "looping": false,
          "time": 5,
          "time_down_event": {
            "event": "minecraft:repeat"
          }
        },
        "minecraft:shooter": {
          "def": "dec:frozen_ball"
        },
        "minecraft:behavior.ranged_attack": {
          "priority": 1,
          "attack_radius": 64,
          "charge_shoot_trigger": 1,
          "charge_charged_trigger": 1
        }
      }
    },
    "components": {
      "minecraft:experience_reward": {
        "on_death": "query.last_hit_by_player ? 5 + (query.equipment_count * Math.Random(1,3)) : 0"
      },
      "minecraft:breathable": {
        "total_supply": 15,
        "suffocate_time": 0
      },
      "minecraft:fire_immune": {},
      "minecraft:nameable": {},
      "minecraft:health": {
        "value": 15,
        "max": 15
      },
      "minecraft:collision_box": {
        "width": 2,
        "height": 2
      },
      "minecraft:movement": {
        "value": 0.02
      },
      "minecraft:navigation.float": {
        "can_path_over_water": true
      },
      "minecraft:behavior.float_wander": {
        "priority": 2,
        "must_reach": true
      },
      "minecraft:can_fly": {},
      "minecraft:type_family": {
        "family": [
          "frozen_ghast",
          "ghast",
          "monster",
          "mob",
          "frozen"
        ]
      },
      "minecraft:damage_sensor": {
        "triggers": [
          {
            "cause": "fall",
            "deals_damage": false
          },
          {
            "on_damage": {
              "filters": {
                "test": "is_family",
                "subject": "other",
                "value": "frozen"
              }
            },
            "deals_damage": false
          }
        ]
      },
      "minecraft:jump.static": {},
      "minecraft:despawn": {
        "despawn_from_distance": {}
      },
      "minecraft:behavior.float": {
        "priority": 0
      },
      "minecraft:follow_range": {
        "value": 64,
        "max": 64
      },
      "minecraft:behavior.hurt_by_target": {
        "priority": 1,
        "entity_types": {
          "filters": {
            "test": "is_family",
            "subject": "other",
            "operator": "!=",
            "value": "frozen"
          },
          "max_dist": 64
        }
      },
      "minecraft:behavior.nearest_attackable_target": {
        "priority": 2,
        "entity_types": [
          {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "value": "player"
            },
            "max_dist": 64
          }
        ],
        "must_see": true
      },
      "minecraft:physics": {},
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      }
    },
    "events": {
      "minecraft:despawn": {
        "add": {
          "component_groups": [
            "minecraft:despawn"
          ]
        }
      },
      "minecraft:from_night_event": {
        "trigger": "minecraft:entity_spawned",
        "add": {
          "component_groups": [
            "minecraft:from_night_event"
          ]
        }
      },
      "minecraft:from_boss": {
        "trigger": "minecraft:entity_spawned",
        "add": {
          "component_groups": [
            "minecraft:from_boss"
          ]
        }
      },
      "minecraft:entity_spawned": {
        "sequence": [
          {
            "randomize": [
              {
                "weight": 1,
                "sequence": [
                  {
                    "randomize": [
                      {
                        "weight": 1,
                        "add": {
                          "component_groups": [
                            "minecraft:repeat"
                          ]
                        }
                      },
                      {
                        "weight": 1,
                        "add": {
                          "component_groups": [
                            "minecraft:single"
                          ]
                        }
                      }
                    ]
                  }
                ]
              }
            ]
          },
          {
            "run_command": {
              "command": [
                "particle dec:frozen_shield_particle ~~~"
              ]
            }
          }
        ]
      },
      "minecraft:single": {
        "remove": {
          "component_groups": [
            "minecraft:repeat"
          ]
        },
        "add": {
          "component_groups": [
            "minecraft:single"
          ]
        }
      },
      "minecraft:repeat": {
        "remove": {
          "component_groups": [
            "minecraft:single"
          ]
        },
        "add": {
          "component_groups": [
            "minecraft:repeat"
          ]
        }
      }
    }
  }
}