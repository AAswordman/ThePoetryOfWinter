{
  "format_version": "1.13.0",
  "minecraft:entity": {
    "description": {
      "identifier": "dec:son_of_nature",
      "runtime_identifier": "minecraft:iron_golem",
      "is_spawnable": false,
      "is_summonable": true,
      "is_experimental": false,
      "scripts": {
        "animate": [
          "disappear"
        ]
      },
      "animations": {
        "disappear": "animation.disappear.10s"
      }
    },
    "component_groups": {
      "minecraft:tame": {
        "minecraft:behavior.owner_hurt_by_target": {
          "priority": 1
        },
        "minecraft:behavior.owner_hurt_target": {
          "priority": 2
        },
        "minecraft:is_tamed": {},
        "minecraft:behavior.follow_owner": {
          "priority": 2,
          "speed_multiplier": 1.0,
          "start_distance": 5,
          "stop_distance": 1
        }
      },
      "minecraft:despawn": {
        "minecraft:instant_despawn": {}
      }
    },
    "components": {
      "minecraft:tameable": {
        "probability": 1,
        "tame_items": [
          "diamond"
        ],
        "tame_event": {
          "event": "minecraft:on_tame",
          "target": "self"
        }
      },
      "minecraft:damage_sensor": {
        "triggers": [
          {
            "cause": "fall",
            "deals_damage": false
          },
          {
            "on_damage": {
              "filters": {
                "all_of": [
                  {
                    "test": "has_damage",
                    "value": "fatal"
                  }
                ]
              },
              "event": "minecraft:disappear"
            },
            "deals_damage": false
          }
        ]
      },
      "minecraft:type_family": {
        "family": [
          "son_of_nature",
          "mob",
          "villager",
          "player"
        ]
      },
      "minecraft:collision_box": {
        "width": 0.7,
        "height": 1.45
      },
      "minecraft:health": {
        "value": 20,
        "max": 20
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": {
              "test": "in_lava",
              "subject": "self",
              "operator": "==",
              "value": true
            },
            "cause": "lava",
            "damage_per_tick": 4
          }
        ]
      },
      "minecraft:movement": {
        "value": 0.2
      },
      "minecraft:navigation.walk": {
        "can_path_over_water": false,
        "avoid_water": true,
        "avoid_damage_blocks": true
      },
      "minecraft:movement.basic": {},
      "minecraft:jump.static": {},
      "minecraft:can_climb": {},
      "minecraft:attack": {
        "damage": {
          "range_min": 1,
          "range_max": 5
        }
      },
      "minecraft:knockback_resistance": {
        "value": 1.0
      },
      "minecraft:balloonable": {},
      "minecraft:behavior.target_when_pushed": {
        "priority": 1,
        "percent_chance": 5.0,
        "entity_types": [
          {
            "filters": {
              "all_of": [
                {
                  "test": "is_family",
                  "subject": "other",
                  "value": "monster"
                }
              ]
            }
          }
        ]
      },
      "minecraft:despawn": {
        "despawn_from_distance": {}
      },
      "minecraft:behavior.melee_attack": {
        "priority": 1,
        "target_dist": 1.0,
        "track_target": true
      },
      "minecraft:behavior.move_towards_target": {
        "priority": 2,
        "speed_multiplier": 0.9,
        "within_radius": 32
      },
      "minecraft:behavior.move_towards_restriction": {
        "priority": 4,
        "speed_multiplier": 1
      },
      "minecraft:behavior.random_stroll": {
        "priority": 6,
        "speed_multiplier": 1,
        "xz_dist": 16
      },
      "minecraft:behavior.look_at_player": {
        "priority": 7,
        "look_distance": 6.0,
        "probability": 0.02
      },
      "minecraft:behavior.random_look_around": {
        "priority": 8
      },
      "minecraft:behavior.nearest_attackable_target": {
        "priority": 3,
        "entity_types": [
          {
            "filters": {
              "all_of": [
                {
                  "test": "is_family",
                  "subject": "other",
                  "value": "monster"
                }
              ]
            },
            "within_default": 10
          }
        ],
        "must_see": true,
        "must_reach": true
