    "events": {
      "minecraft:disappear": {
        "queue_command": {
          "command": [
            "particle dec:summon_staff_spawn_disappear ~~~",
            "event entity @s minecraft:despawn"
          ]
        }
      },
      "minecraft:on_tame": {
        "add": {
          "component_groups": [
            "minecraft:tame"
          ]
        }
      },
      "minecraft:despawn": {
        "add": {
          "component_groups": [
            "minecraft:despawn"
          ]
        }
      }
    }
  }
